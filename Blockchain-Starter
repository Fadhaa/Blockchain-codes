    "def gettrans(iz):\n",
    "    htt=urllib3.PoolManager()\n",
    "    ml = htt.request('GET', 'https://chain.api.btc.com/v3/tx/unconfirmed')\n",
    "    ml=ml.data\n",
    "    da=str(ml)\n",
    "    da=da[-13800:]\n",
    "    newda=da[(da.index(',\"')+1):da.index(']},\"err_code\"')]\n",
    "    fn=open('/kaggle/working/tx'+str(iz)+'.txt','w')\n",
    "    fn.write(newda)\n",
    "    fn.close()\n",
    "    return newda"
   ]
